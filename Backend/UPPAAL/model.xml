  <template>
    <name>Scheduler</name>
    <location id="Scheduler_Idle">
      <name>Idle</name>
    </location>
    <location id="Scheduler_Dispatch">
      <name>Dispatch</name>
      <committed />
    </location>
    <location id="Scheduler_Post">
      <name>Post</name>
      <committed />
    </location>
    <location id="Scheduler_Busy">
      <name>Busy</name>
    </location>
    <init ref="Scheduler_Idle" />
    <transition>
      <source ref="Scheduler_Idle" />
      <target ref="Scheduler_Dispatch" />
      <label kind="synchronisation">release_Perception_A?</label>
    </transition>
    <transition>
      <source ref="Scheduler_Idle" />
      <target ref="Scheduler_Dispatch" />
      <label kind="synchronisation">release_AckHandler_A?</label>
    </transition>
    <transition>
      <source ref="Scheduler_Idle" />
      <target ref="Scheduler_Dispatch" />
      <label kind="synchronisation">release_Perception_B?</label>
    </transition>
    <transition>
      <source ref="Scheduler_Idle" />
      <target ref="Scheduler_Dispatch" />
      <label kind="synchronisation">release_PermissionReqTx_B?</label>
    </transition>
