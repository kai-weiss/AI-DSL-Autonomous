      <target ref="Scheduler_Dispatch" />
      <label kind="synchronisation">release_Controller_B?</label>
    </transition>
    <transition>
      <source ref="Scheduler_Dispatch" />
      <target ref="Scheduler_Busy" />
      <label kind="guard">running == -1 &amp;&amp; ready[IDX_PermissionReqTx_B]</label>
      <label kind="synchronisation">start_PermissionReqTx_B!</label>
      <label kind="assignment">running = IDX_PermissionReqTx_B, ready[IDX_PermissionReqTx_B] = false</label>
    </transition>
    <transition>
      <source ref="Scheduler_Dispatch" />
      <target ref="Scheduler_Busy" />
      <label kind="guard">running == -1 &amp;&amp; ready[IDX_AckHandler_A] &amp;&amp; !ready[IDX_PermissionReqTx_B]</label>
      <label kind="synchronisation">start_AckHandler_A!</label>
      <label kind="assignment">running = IDX_AckHandler_A, ready[IDX_AckHandler_A] = false</label>
    </transition>
    <transition>
      <source ref="Scheduler_Dispatch" />
      <target ref="Scheduler_Busy" />
      <label kind="guard">running == -1 &amp;&amp; ready[IDX_PermissionAckRx_B] &amp;&amp; !ready[IDX_PermissionReqTx_B] &amp;&amp; !ready[IDX_AckHandler_A]</label>
      <label kind="synchronisation">start_PermissionAckRx_B!</label>
      <label kind="assignment">running = IDX_PermissionAckRx_B, ready[IDX_PermissionAckRx_B] = false</label>
    </transition>
    <transition>
