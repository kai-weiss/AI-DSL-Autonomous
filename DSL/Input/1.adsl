COMPONENT CameraProcessing {
    period   = 33ms;
    WCET     = 20ms;
    priority = 3;
}

COMPONENT PathPlanning {
    period   = 60ms;
    deadline = 90ms;
    WCET     = 30ms;
    priority = 2;
}
CONNECT CameraProcessing.output -> PathPlanning.input {
    latency_budget = 90ms;
}
PROPERTY EndToEndLatency: "CameraProcessing to PathPlanning response within 100 ms";